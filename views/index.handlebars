{{!-- NavBar --}}
<nav class="navbar navbar-light bg-light justify-content-between">
  <img  src="/assets/img/logo-sf-small.png" alt="">
  <form class="form-inline">
    <button class="btn btn-outline-success my-2 my-sm-0" type="submit">Message Viewer</button>
  </form>
</nav>
{{!-- End --}}

{{!-- Card Message --}}
<div class="container-fluid">
  <div class="row">
    <div class="col-12">
      <h2>Starred:</h2>
      <div id="buttonParent">
        <input class="btn btn-primary" type="button" value="Show Trashed Messages" id="btn1"/> <input class="btn btn-primary" type="button" value="Score" id="score"/> 
      </div>
      <div class="divTrash">
        {{#each messages}}
          {{#if isTrashed}}
            {{> messageIsTrashed/messageTrash }}
          {{/if}}
        {{/each}}
      </div>
      
      <div class="divUntrash">
        {{#each messages}}
        {{#unless isTrashed}}
            {{> messagesIsUntrashed/messageUntrash }}
          {{/unless}}
        {{/each}}
      </div>
    </div>
  </div>
</div>
{{!-- End --}}